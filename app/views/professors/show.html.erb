<section id="professor-<%= @professor.id %>">
  <h1>Professor:</h1>
  <p>Name: <%= @professor.name %></p>
  <p>Age: <%= @professor.age %></p>
  <p>Specialty: <%= @professor.specialty %></p>
  <p>Students Average Age: <%= @professor.students.average_age %></p>
  <h2>Students: </h2>
  <li class='student_list'>
    <% @professor.students.each do |student| %>
      <p><%= student.name %>
      <%= button_to "Unenroll", "/professors/#{@professor.id}?unenroll_student_id=#{student.id}" %></p>
    <% end %>
  </li>
</section>
